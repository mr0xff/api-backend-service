{
  "type": "module",
  "name": "api-service",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mr0xff/api-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mr0xff/api-service/issues"
  },
  "homepage": "https://github.com/mr0xff/api-service#readme",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/env": "^5.0.3",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "6.10.1",
    "fastify": "^5.3.2",
    "fastify-cli": "^7.3.0",
    "fastify-plugin": "^5.0.0",
    "mongoose": "^8.16.0",
    "prisma": "^6.10.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.6.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
